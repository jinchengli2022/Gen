<mujoco model="yellow_cup">
  <asset>
    <mesh file="meshes/yellow_cup/yellow_cup.obj" name="yellow_cup_mesh" scale="0.001 0.001 0.001"/>
    <texture file="meshes/yellow_cup/Image_0.png" name="tex-yellow-cup" type="2d"/>
    <material name="yellow_cup_mat" reflectance="0.5" texrepeat="1 1" texture="tex-yellow-cup"/>
  </asset>
  <worldbody>
    <body>
      <body name="object">
        <geom pos="0 0 0" mesh="yellow_cup_mesh" type="mesh" solimp="0.998 0.998 0.001" solref="0.001 1" density="1000" friction="0.95 0.3 0.1" group="0" condim="4" material="yellow_cup_mat"/>
      </body>
      <!-- 标记物体的底部接触点、中心点以及水平边缘点，用于碰撞检测等 -->
      <site rgba="0 0 0 0" size="0.005" pos="0 0 -0.03" name="bottom_site"/>
      <site rgba="0 0 0 0" size="0.005" pos="0 0 0.03" name="top_site"/>
      <site rgba="0 0 0 0" size="0.005" pos="0.076 0 0" name="horizontal_radius_site"/>
    </body>
  </worldbody>
</mujoco>
